{
  "version": "1.0.0-*",
  "testRunner": "nunit",
  "dependencies": {
    "dotnet-test-nunit": "3.4.0-beta-3",
    "NUnit": "3.5.0",
    "Moq": "4.6.38-*",
    "NUnit.Console": "3.5.0",
    "Ruzzie.FuzzyStrings": "1.0.0-*"
  },
  "frameworks": {   
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        },
        "System.Diagnostics.Process": "4.1.0"
      }
    },   
    "net46": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_VISUALBASIC_DEVICES" ]
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": ""
      },
      "dependencies": {
      }
    },
    "net461": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_VISUALBASIC_DEVICES", "HAVE_PARALLELPERFORMANCE" ]
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": ""
      },
      "dependencies": {      
      }
    }    
  },
  "buildOptions": {
    "optimize": true
  }
}
