{
  "version": "1.0.0-*",
  "title": "Ruzzie.FuzzyStrings",
  "description": "A collection of fuzzy string algorithms for .net",
  "language": "en-US",
  "authors": [ "Dorus Verhoeckx" ],
  "copyright": "Copyright � Dorus Verhoeckx 2017",
  "packOptions": {
    "projectUrl": "https://github.com/Ruzzie/Ruzzie.Common",
    "licenseUrl": "https://github.com/Ruzzie/fuzzystrings/blob/master/src/Ruzzie.FuzzyStrings/License.txt",
    "repository": {
      "type": "git",
      "url": "git://github.com/Ruzzie/Ruzzie.Common"
    }
  },
  "dependencies": {
    "Ruzzie.Cache.FlashCache": "2.0.100"
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.1",
        "System.Diagnostics.Process": "4.1.0"
      }
    },
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        },
        "System.Diagnostics.Process": "4.1.0"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_VISUALBASIC_DEVICES" ]
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": ""
      }
    },
    "net46": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_VISUALBASIC_DEVICES" ]
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": ""
      },
      "dependencies": {
      }
    },
    "net461": {
      "buildOptions": {
        "define": [ "HAVE_SERIALIZABLE", "HAVE_VISUALBASIC_DEVICES" ]
      },
      "frameworkAssemblies": {
        "Microsoft.VisualBasic": ""
      },
      "dependencies": {      
      }
    },
    ".NETPortable,Version=v4.0,Profile=Profile344": {
      "buildOptions": {
        "define": [ "PORTABLE" ]
      },
      "frameworkAssemblies": {
      
        "Microsoft.CSharp": "2.0.5.0",
        "mscorlib": "2.0.5.0",
        "System": "2.0.5.0",
        "System.Core": "2.0.5.0"
      }
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "optimize": true,
    "nowarn": [ "1591" ]
  }
}
